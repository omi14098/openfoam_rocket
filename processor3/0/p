FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101325;

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    rocket
    {
        type            zeroGradient;
    }
    walls
    {
        type            zeroGradient;
    }
    procBoundary3to1
    {
        type            processor;
        value           uniform 101325;
    }
    procBoundary3to2
    {
        type            processor;
        value           uniform 101325;
    }
}


