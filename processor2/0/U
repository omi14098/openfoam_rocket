FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (120 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (120 0 0);
    }
    outlet
    {
        type            zeroGradient;
    }
    rocket
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    walls
    {
        type            zeroGradient;
    }
    procBoundary2to0
    {
        type            processor;
        value           uniform (120 0 0);
    }
    procBoundary2to1
    {
        type            processor;
        value           uniform (120 0 0);
    }
    procBoundary2to3
    {
        type            processor;
        value           uniform (120 0 0);
    }
}


