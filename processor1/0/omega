FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 638;

boundaryField
{
    inlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    rocket
    {
        type            omegaWallFunction;
        value           uniform 638;
    }
    walls
    {
        type            zeroGradient;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 638;
    }
    procBoundary1to2
    {
        type            processor;
        value           uniform 638;
    }
    procBoundary1to3
    {
        type            processor;
        value           uniform 638;
    }
}


